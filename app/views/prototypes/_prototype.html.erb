<div class="card">
  <%= link_to image_tag(prototype.image, class: :card_img ), prototype_path(prototype) %>
  <div class="card_body">
    <%= link_to prototype.title, prototype_path(prototype), class: :card_title%>
    <p class="card_summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", user_path(prototype.user), class: :card_user %>
  </div>
</div>